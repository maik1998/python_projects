    "def findPosById (idUser):\n",
    "    posId=0\n",
    "    for key,value in usuarioDic.items():\n",
    "            if idUser == value:\n",
    "                 posId= key\n",
    "    return posId\n",
    " \n",
    "for i in range(len(rating)):\n",
    "        userPos=findPosById(rating[i,0])-1\n",
    "        moviePos=rating[i,1]-1\n",
    "        ranking=rating[i,2]\n",
    "        \n",
    "        if moviePos<2000:\n",
    "             X[userPos,moviePos]=ranking\n",
    "\n",
    "\n",
    "print(X)\n",
    "\n"
   ]
