   "source": [
    "Se construye el conjunto de entrenamiento (matriz) usando todos los usuarios del archivo personality-data.csv y las primeras 2000 películas. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 261,
   "metadata": {
    "scrolled": false
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(1834, 2000)\n"
     ]
    }
   ],
   "source": [
    "#Se extraen los valores del Dataframe\n",
    "id_usuarioP=dfp.values[:,:1]\n",
    "id_pelicula=dfr.values[0:2000,1:2]\n",
    "rating=dfr.values[:,0:3]\n",
    "\n",
    "#Se crea un arreglo numpy de usuario\n",
    "usuario=np.array(id_usuarioP)\n",
    "\n",
    "#usuario.flatten : se utiliza para obtener una copia de la matriz dada, contraída en una dimensión.\n",
    "usuarioDic=dict(enumerate(usuario.flatten(),1))\n",
    "\n",
    "#print(rating[2,0])\n",
    "#idU= id_usuario.to_dict('index')\n",
    "#print(idU[0].values())\n",
    "\n",
    "X = np.zeros((len(id_usuarioP),len(id_pelicula)))\n",
    "print(X.shape)\n",
    "#Xaux=np.zeros((len(id_usuario),len(rating)) \n"
   ]
